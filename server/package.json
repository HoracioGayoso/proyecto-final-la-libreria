{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "start:dev": "nodemon src/index.ts",
    "knex:migrate": "knex migrate:latest --knexfile ./knexfile.ts",
    "knex:migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.ts",
    "knex:seed": "knex seed:run --knexfile ./knexfile.ts",
    "knex:migrate:make": "knex migrate:make --knexfile ./knexfile.ts",
    "knex:seed:make": "knex seed:make --knexfile ./knexfile.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "firebase-admin": "^12.3.0",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.6",
    "axios": "^1.7.3",
    "firebase": "^10.12.5",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
